{
    "name": "weirweb/engine-routing",
    "description": "Weirweb Engine - Routing Module",
    "version": "1.0",
    "type": "package",
    "require-dev": {
        "phpstan/phpstan": "^0.12.81",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Engine\\Routing\\": "src/"
        }
    },
    "require": {
        "weirweb/engine": "@dev",
        "weirweb/engine-page": "@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../engine"
        },
        {
            "type": "path",
            "url": "../engine-page"
        }
    ]
}